package com.tutorial.rajib;

public class StringPractice2 {
    static String str = "Raj";
   int j = 20;
  static  int k = 5;
	static int occurance(String s, char c) {
        System.out.println(str);
        StringPractice2 s4 = new StringPractice2();
        System.out.println(s4.j);
        System.out.println(k);
		int count =0;
		for(int i=0; i<s.length();i++ ) {

			if( s.charAt(i)==c) {
				count = count+1;
			}
		}
		return count;

	}
	public static void main(String[] args) {

		String input=" Hello World";
		System.out.println( occurance(input, 'l')); 
		System.out.println(str);
		
		StringPractice2 s2 = new StringPractice2();
		System.out.println(s2.j);
	}
	
}